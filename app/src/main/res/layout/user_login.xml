<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF5E9"
    tools:context=".user_login">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/studentwelcome" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="145dp"
        android:layout_height="65dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/medium"
        android:text="Welcome Back"
        android:textAlignment="textStart"
        android:textColor="#343330"
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.199"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginBottom="15dp"
        android:fontFamily="@font/regular"
        android:text="We could hear your stomach from here ;D"
        android:textAlignment="textStart"
        android:textColor="#343330"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.277"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="35dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mail" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout3"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout2"
        app:layout_constraintEnd_toEndOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/u_loginemail"
            android:layout_width="280dp"
            android:layout_height="40dp"
            android:layout_marginEnd="48dp"
            android:layout_marginBottom="15dp"
            android:ems="10"
            android:fontFamily="@font/regular"
            android:hint="email"
            android:inputType="textEmailAddress"
            android:textColor="#343330"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent" />
    </com.google.android.material.textfield.TextInputLayout>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/u_forgotpassword"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/lock" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout2"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/u_forgotpassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:passwordToggleEnabled="true"
        app:passwordToggleTint="@color/password">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/u_loginpassword"
            android:layout_width="280dp"
            android:layout_height="40dp"
            android:layout_marginEnd="48dp"
            android:layout_marginBottom="15dp"
            android:ems="10"
            android:fontFamily="@font/regular"
            android:hint="password"
            android:inputType="textPassword"
            android:textColor="#343330"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/u_forgotpassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="55dp"
        android:fontFamily="@font/regular"
        android:text="Forgot Password?"
        android:textColor="#D9381E"
        android:textSize="11sp"
        app:layout_constraintBottom_toTopOf="@+id/u_loginbutton"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/u_loginbutton"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:background="@drawable/bluebutton"
        android:fontFamily="@font/regular"
        android:text="Login"
        android:textColor="#FFF8F0"
        android:textSize="13sp"
        app:layout_constraintBottom_toTopOf="@+id/alreadyhaveaccount2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/alreadyhaveaccount2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="81dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/regular"
        android:text="Don't have an account?"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/u_signuphere"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/regular"
        android:text="Sign up here"
        android:textColor="#D9381E"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/alreadyhaveaccount2" />

</androidx.constraintlayout.widget.ConstraintLayout>