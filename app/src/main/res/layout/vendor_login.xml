<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF5E9"
    tools:context=".vendor_login">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/vendorlogin" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="145dp"
        android:layout_height="65dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/medium"
        android:text="Welcome Back"
        android:textAlignment="textStart"
        android:textColor="#343330"
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="125dp"
        android:layout_height="37dp"
        android:layout_marginBottom="10dp"
        android:fontFamily="@font/regular"
        android:text='"Mise en place"'
        android:textAlignment="textStart"
        android:textColor="#343330"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.209"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="35dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout3"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/mail" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout4"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout3"
        app:layout_constraintEnd_toEndOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/v_loginemail"
            android:layout_width="280dp"
            android:layout_height="40dp"
            android:layout_marginEnd="48dp"
            android:layout_marginBottom="15dp"
            android:ems="10"
            android:fontFamily="@font/regular"
            android:hint="email"
            android:inputType="textEmailAddress"
            android:textColor="#343330"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent" />
    </com.google.android.material.textfield.TextInputLayout>

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="70dp"
        app:layout_constraintBottom_toTopOf="@+id/v_loginbutton"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/lock" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout3"
        android:layout_width="280dp"
        android:layout_height="50dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="70dp"
        app:layout_constraintBottom_toTopOf="@+id/v_loginbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:passwordToggleEnabled="true"
        app:passwordToggleTint="@color/password">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/v_loginpassword"
            android:layout_width="280dp"
            android:layout_height="40dp"
            android:layout_marginEnd="48dp"
            android:layout_marginBottom="15dp"
            android:ems="10"
            android:fontFamily="@font/regular"
            android:hint="password"
            android:inputType="textPassword"
            android:textColor="#343330"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent" />
    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/v_loginbutton"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="60dp"
        android:background="@drawable/bluebutton"
        android:fontFamily="@font/regular"
        android:text="Login"
        android:textColor="#FFF8F0"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>